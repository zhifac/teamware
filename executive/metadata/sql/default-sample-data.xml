<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE dataset [
  <!ENTITY superOnly '<tw:role name="superadmin"/>'>
  <!ENTITY saAndAdmin '&superOnly;<tw:role name="admin"/>'>
  <!ENTITY adminAndMgr '&saAndAdmin;<tw:role name="manager"/>'>
  <!ENTITY allRoles '&adminAndMgr;<tw:role name="annotator"/>'>
]>
<!-- Sample data for the teamware database.  This is the default data set,
     there will also be installation-specific data in the installations/*
     directory -->
<dataset xmlns:tw="http://gate.ac.uk/ns/teamware/sample-data">
  <role name="superadmin"
        description="Super user, in charge of the entire system"/>
  <role name="admin"
        description="Administrator, in charge of the users, resources and projects"/>
  <role name="manager" description="Manage workflows and projects"/>
  <role name="annotator" description="Can only annotate"/>

  <!-- Application users - by default there's just the "super-admin" and admin
  -->
  <app_user version="1" username="superadmin"
            password="teamware"
            iterations="0"
            password_hint="teamware" first_name="SuperAdmin"
            last_name="SuperAdmin" email="teamware@example.com"
            phone_number="0044-1142221800" website="http://gate.ac.uk"
            address="Regent Court, 211 Portobello" city="Sheffield"
            province="SY" country="GB" postal_code="S14DP" account_enabled="Y"
            account_expired="N" account_locked="N" credentials_expired="N">
    <tw:role name="superadmin" />
  </app_user>

  <app_user version="1" username="admin"
            password="teamware"
            iterations="0"
            password_hint="teamware" first_name="Admin"
            last_name="Admin" email="teamware@example.com"
            phone_number="0044-1142221800" website="http://gate.ac.uk"
            address="Regent Court, 211 Portobello" city="Sheffield"
            province="SY" country="GB" postal_code="S14DP" account_enabled="Y"
            account_expired="N" account_locked="N" credentials_expired="N">
    <tw:role name="admin" />
    <tw:role name="manager" />
    <tw:role name="annotator" />
  </app_user>

  <!-- Services (used to group actions together) -->
  <service name="Admin Service" service_enabled="Y"/>
  <service name="User Service" service_enabled="Y"/>
  <service name="Common Service" service_enabled="Y"/>
  <service name="Help" service_enabled="Y"/>
  <service name="Documents Service" service_enabled="Y"/>
  <service name="Workflow Service" service_enabled="Y"/>
  <service name="Annotation Service" service_enabled="Y"/>
  <service name="Ontologies Service" service_enabled="Y"/>

  <!-- By default we only support one type of annotation service -->
  <annotation_service_type name="gas"
      data="asKey,asValue,asExtraMappings,parameterKey,parameterValue"/>

  <!-- No annotation services available by default - add extras in
       installation-specific data file -->

  <!-- Resources and their role mappings (see entities at the top of this file
       for shorthands) -->

  <!-- Admin service -->
  <resource url="/aMenu.html*" description="Admin Menu"
            tw:service="Admin Service">&saAndAdmin;</resource>
  <resource url="/activeUsers.html*" description="list all active users"
            tw:service="Admin Service">&adminAndMgr;</resource>
  <resource url="/roles.html*" description="Roles List Page"
            tw:service="Admin Service">&superOnly;</resource>
  <resource url="/editRole.html*" description="Edit Role"
            tw:service="Admin Service">&superOnly;</resource>
  <resource url="/reload.html*" description="Reload Page"
            tw:service="Admin Service">&saAndAdmin;</resource>
  <resource url="/flush.html*" description="Flush Cache Page"
            tw:service="Admin Service">&saAndAdmin;</resource>
  <resource url="/clickstreams.jsp*" description="Click Streams Page"
            tw:service="Admin Service">&saAndAdmin;</resource>
  <!-- Only super-admin can modify resource definitions (these role->URL
       mappings) and create new services -->
  <resource url="/resources.html*" description="Resources List Page"
            tw:service="Admin Service">&superOnly;</resource>
  <resource url="/services.html*" description="Services List"
            tw:service="Admin Service">&superOnly;</resource>

  <!-- Annotation service -->
  <resource url="/resourceMenu.html*" description="Resource Menu"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/annotationServices.html*"
            description="Annotation Services"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/annotationServiceInfo.html*"
            description="Annotation Service Info"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/popupAnnotationServiceInfo.html*"
            description="Annotation Service Info"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/editAnnotationService.html*"
            description="Edit Annotation Service"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/saveAnnotationService.html*"
            description="Save Annotation Service"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/popupAddAnnotationService.html*"
            description="Add Annotation Service"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/schemas.html*" description="Annotation Schemas"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/addSchema.html*" description="Add annotation schema"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/popupAddSchema.html*" description="Add annotation schema"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/schemaUploadInfo.html*"
            description="Schema upload info page"
            tw:service="Annotation Service">&adminAndMgr;</resource>
  <resource url="/popupSchemaUploadInfo.html*"
            description="Schema upload info page"
            tw:service="Annotation Service">&adminAndMgr;</resource>

  <!-- "pseudo-resource" used to determine which roles should be shown the
       pool-mode annotator GUI link on the front page -->
  <resource url="/annotator-gui/*" description="Annotator GUI URL"
            tw:service="Annotation Service">
    <tw:role name="annotator"/>
  </resource>

  <!-- Common service and help-related stuff -->
  <resource url="/mainMenu.html*" description="Main Menu Page"
            tw:service="Common Service">&allRoles;</resource>
  <resource url="/helpInfo.html*" description="Help Page"
            tw:service="Help">&allRoles;</resource>
  <resource url="/forum.html*" description="Forum"
            tw:service="Help">&allRoles;</resource>
  <resource url="/supportMenu.html*" description="Support Menu"
            tw:service="Common Service">&allRoles;</resource>
  <resource url="/chat.html*" description="Chat"
            tw:service="Help">&allRoles;</resource>
  <resource url="/scripts/util.jsp*" description="DWR Util Page"
            tw:service="Common Service">&allRoles;</resource>
  <resource url="/scripts/engine.jsp*" description="Engine Page"
            tw:service="Common Service">&allRoles;</resource>
  <resource url="/download.html*" description="Download"
            tw:service="Help">&allRoles;</resource>
  <!-- Only admin can access forum administration -->
  <resource url="/adm*" description="Forum Administration"
            tw:service="Help">&saAndAdmin;</resource>

  <!-- Documents service -->
  <resource url="/corpora.html*" description="DocService Home Page"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/editUploadCorpus.html*" description="Upload Corpus"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/saveUploadCorpus.html*" description="Upload Corpus"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/corpusUploadInfo.html*" description="Corpus upload info"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/popupCorpusUploadInfo.html*" description="Corpus upload info"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/saveCorpus.html*" description="Save Corpus"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/documentsInCorpus.html*" description="Documents in corpus"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/datastore.html*"
            description="Download Datastore"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/corpus.html*"
            description="Download Corpus"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/annicgui.html*" description="ANNIC GUI"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/viewDocument.html*"
            description="View document in annotator GUI"
            tw:service="Documents Service">&adminAndMgr;</resource>
  <resource url="/annotatorGui.html*"
            description="Launch pool-mode Annotator GUI"
            tw:service="Documents Service">&allRoles;</resource>

  <!-- Ontologies service -->
  <resource url="/ontologyRepositoryList.html*"
            description="Ontology Repository List"
            tw:service="Ontologies Service">&saAndAdmin;</resource>
  <resource url="/addOntologyRepository.html*"
            description="Add Ontology Repository"
            tw:service="Ontologies Service">&saAndAdmin;</resource>

  <!-- User service -->
  <resource url="/editProfile.html*"
            description="User Profile Edit Page"
            tw:service="User Service">&allRoles;</resource>
  <resource url="/passwordHint.html*" description="Password Hint Page"
            tw:service="User Service">&allRoles;</resource>
  <resource url="/signup.html*" description="SignUp Page"
            tw:service="User Service">&allRoles;</resource>
  <resource url="/users.html*" description="Users List Page"
            tw:service="User Service">&saAndAdmin;</resource>
  <resource url="/editUser.html*" description="Users Edit Page"
            tw:service="User Service">&saAndAdmin;</resource>
  <resource url="/bulkUpload.html*" description="Bulk upload form"
            tw:service="User Service">&saAndAdmin;</resource>
  <resource url="/bulkSave.html*" description="Bulk upload target"
            tw:service="User Service">&saAndAdmin;</resource>

  <!-- Workflow service -->
  <!-- Note that by default only superadmin can modify process definitions -->
  <resource url="/processDefinitionList.html?method=list*"
            description="Process Definitions List Page"
            tw:service="Workflow Service">&superOnly;</resource>
  <resource url="/processDefinitionUpload.html*"
            description="Upload Process Definition"
            tw:service="Workflow Service">&superOnly;</resource>
  <resource url="/processInstanceList.html*"
            description="Process Instance List Page"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <!-- Following specific mapping is required to make menus work -->
  <resource url="/processInstanceList.html?method=listAll*"
            description="Running Projects"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <resource url="/editActors.html*"
            description="Edit actors for process instance"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <resource url="/saveActors.html*"
            description="Save actors for process instance"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <resource url="/taskInstanceList.html?method=list*"
            description="Task Instance List Page"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <resource url="/taskInstanceList.html?method=listByActor*"
            description="Pending Task Instance Page"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <resource url="/workflowMenu.html*" description="Work Flow Home Page"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <resource url="/editProject.html*" description="Edit Project"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <resource url="/loadProject.html?method=load*"
            description="New Process Instance"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <resource url="/projects.html*"
            description="Workflow Templates"
            tw:service="Workflow Service">&adminAndMgr;</resource>
  <!-- Following specific mapping is required to make menus work -->
  <resource url="/projects.html?method=listAll*"
            description="All Workflow Templates"
            tw:service="Workflow Service">&adminAndMgr;</resource>

  <!-- Catch-all for anything not further restricted above -->
  <resource url="/**/*.html*" description="All the Pages"
            tw:service="Common Service">&allRoles;</resource>

