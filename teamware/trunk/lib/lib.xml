  <!--
   Macro that causes the build to fail if the given library is not available.
   Typically used to check that a library that is created by another SAFE
   component has been built before this project.

   <require lib="safe-common" />
  -->
  <macrodef name="require">
    <attribute name="lib" />
    <sequential>
      <pathconvert property="@{lib}.exists" setonempty="false">
        <path>
          <fileset dir="${lib.dir}">
            <patternset refid="pattern.@{lib}" />
          </fileset>
        </path>
      </pathconvert>
      <fail unless="@{lib}.exists" message="Library @{lib} not available!" />
    </sequential>
  </macrodef>
</project>

